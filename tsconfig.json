{
  "compilerOptions": {
    "target": "es2016",
    "lib": ["dom", "es2016"],
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types", "./Types"],
    "resolveJsonModule": true
  },
  "include": [
    "./src/**/*",
    "./Types",
    "modules/projects.js",
    "modules/skills.js"
  ]
}
